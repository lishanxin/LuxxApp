<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffdddddd" >

    <net.senink.seninkapp.ui.view.TopNavArea
        android:id="@+id/icon_search_top"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_alignParentTop="true" />

    <net.senink.seninkapp.ui.view.BottomNavArea
        android:id="@+id/icon_search_bottom"
        android:layout_width="match_parent"
        android:layout_height="60dip"
        android:layout_alignParentBottom="true"
        android:contentDescription="setting" />

    <ScrollView
        android:id="@+id/content_title_area"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/icon_search_bottom"
        android:layout_below="@id/icon_search_top" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/control_lebal"
                style="@style/TextStyleInverse"
                android:layout_height="30dip"
                android:layout_marginLeft="24dp"
                android:text="@string/contal" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <RelativeLayout
                android:id="@+id/parent_control_area"
                style="@style/TitleLayoutStyle"
                android:background="@android:color/white" >

                <TextView
                    android:id="@+id/parent_control_lebal"
                    style="@style/TextStyleInverse"
                    android:layout_marginLeft="24dp"
                    android:drawableLeft="@drawable/ic_launcher" />

                <Button
                    android:id="@+id/parent_control_right"
                    style="@style/MenuInTitleStyle"
                    android:text="@string/install" />

                <TextView
                    style="@style/TextStyleInverse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dip"
                    android:layout_toLeftOf="@id/parent_control_right"
                    android:layout_toRightOf="@id/parent_control_lebal"
                    android:text="@string/parent_contal"
                    android:textSize="15dip" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <RelativeLayout
                android:id="@+id/flow_area"
                style="@style/TitleLayoutStyle"
                android:background="@android:color/white" >

                <TextView
                    android:id="@+id/flow_lebal"
                    style="@style/TextStyleInverse"
                    android:layout_marginLeft="24dp"
                    android:drawableLeft="@drawable/ic_launcher" />

                <Button
                    android:id="@+id/flow_right"
                    style="@style/MenuInTitleStyle"
                    android:text="@string/install" />

                <TextView
                    style="@style/TextStyleInverse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/flow_right"
                    android:layout_toRightOf="@id/flow_lebal"
                    android:text="@string/liu_liang_contal"
                    android:textSize="15dip" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <RelativeLayout
                android:id="@+id/broadband_area"
                style="@style/TitleLayoutStyle"
                android:background="@android:color/white" >

                <TextView
                    android:id="@+id/broadband_lebal"
                    style="@style/TextStyleInverse"
                    android:layout_marginLeft="24dp"
                    android:drawableLeft="@drawable/ic_launcher" />

                <Button
                    android:id="@+id/broadband_right"
                    style="@style/MenuInTitleStyle"
                    android:text="@string/install" />

                <TextView
                    style="@style/TextStyleInverse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/broadband_right"
                    android:layout_toRightOf="@id/broadband_lebal"
                    android:text="@string/dai_kuai_control"
                    android:textSize="15dip" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <TextView
                android:id="@+id/speed_lebal"
                style="@style/TextStyleInverse"
                android:layout_height="30dip"
                android:layout_marginLeft="24dp"
                android:text="@string/jia_su" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <RelativeLayout
                android:id="@+id/video_area"
                style="@style/TitleLayoutStyle"
                android:background="@android:color/white" >

                <TextView
                    android:id="@+id/video_lebal"
                    style="@style/TextStyleInverse"
                    android:layout_marginLeft="24dp"
                    android:drawableLeft="@drawable/ic_launcher" />

                <Button
                    android:id="@+id/video_right"
                    style="@style/MenuInTitleStyle"
                    android:text="@string/install" />

                <TextView
                    style="@style/TextStyleInverse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dip"
                    android:layout_toLeftOf="@id/video_right"
                    android:layout_toRightOf="@id/video_lebal"
                    android:text="@string/video_jia_su"
                    android:textSize="15dip" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <RelativeLayout
                android:id="@+id/game_area"
                style="@style/TitleLayoutStyle"
                android:background="@android:color/white" >

                <TextView
                    android:id="@+id/game_lebal"
                    style="@style/TextStyleInverse"
                    android:layout_marginLeft="24dp"
                    android:drawableLeft="@drawable/ic_launcher" />

                <Button
                    android:id="@+id/game_right"
                    style="@style/MenuInTitleStyle"
                    android:text="@string/install" />

                <TextView
                    style="@style/TextStyleInverse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/game_right"
                    android:layout_toRightOf="@id/game_lebal"
                    android:text="@string/game_jia_su"
                    android:textSize="15dip" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />

            <RelativeLayout
                android:id="@+id/terminal_area"
                style="@style/TitleLayoutStyle"
                android:background="@android:color/white" >

                <TextView
                    android:id="@+id/terminal_lebal"
                    style="@style/TextStyleInverse"
                    android:layout_marginLeft="24dp"
                    android:drawableLeft="@drawable/ic_launcher" />

                <Button
                    android:id="@+id/terminal_right"
                    style="@style/MenuInTitleStyle"
                    android:text="@string/install" />

                <TextView
                    style="@style/TextStyleInverse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/terminal_right"
                    android:layout_toRightOf="@id/terminal_lebal"
                    android:text="@string/zhong_duan_jia_su"
                    android:textSize="15dip" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="#B6B6B6" />
        </LinearLayout>
    </ScrollView>

</RelativeLayout>